algoritmo "semnome"
// Função :
// Autor :
// Data : 18/07/2023
// Seção de Declarações 
var
 nota1, nota2: real
inicio
// Seção de Comandos 
   nota1 := 10
   nota2 := 15
   
   
   se(nota1 <> nota2) entao
   escreval ("Nota1 e diferente")
   senao
   escreval ("Nota1 e igual")
   fimse


fimalgoritmo
