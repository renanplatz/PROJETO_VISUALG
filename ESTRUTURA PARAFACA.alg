algoritmo "semnome"
// Função :
// Autor :
// Data : 20/07/2023
// Seção de Declarações 
var
numero, fatorial, contador: inteiro
inicio
// Seção de Comandos 
escreva ("Digite o numero para calcular o fatorial.")
leia(numero)
fatorial := 1
para contador de 1 ate numero faca
     fatorial := fatorial * contador
fimpara
escreva("O fatorial de", numero, " é: ", fatorial)
fimalgoritmo
